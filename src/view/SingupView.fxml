<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="480.0" prefWidth="720.0" styleClass="singupBG" stylesheets="@../assets/Style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SingupViewController">

    <children>
        <Label fx:id="closeButton" layoutX="3.0" layoutY="4.0" onMouseClicked="#closeAct" prefHeight="25.0" prefWidth="25.0" text="x">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Label>
      <Pane layoutX="49.0" layoutY="60.0" prefHeight="420.0" prefWidth="620.0" styleClass="singupPane">
         <children>
            <Pane layoutX="135.0" layoutY="71.0" prefHeight="240.0" prefWidth="350.0">
               <children>
                  <TextField fx:id="emailField" layoutX="2.0" prefHeight="40.0" prefWidth="350.0" promptText="jarbas@alunos.utfpr.edu.br" styleClass="textfieldLightBox">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </TextField>
                  <TextField fx:id="raField" layoutX="2.0" layoutY="65.0" prefHeight="40.0" prefWidth="350.0" promptText="RA" styleClass="textfieldLightBox">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </TextField>
                  <PasswordField fx:id="passField" layoutX="2.0" layoutY="130.0" prefHeight="40.0" prefWidth="350.0" promptText="******" styleClass="textfieldLightBox">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </PasswordField>
                  <PasswordField fx:id="passConfField" layoutY="194.0" prefHeight="40.0" prefWidth="350.0" promptText="******" styleClass="textfieldLightBox">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </PasswordField>
               </children>
            </Pane>
            <Button fx:id="cancelButton" onMouseClicked="#backAct" layoutX="135.0" layoutY="360.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="175.0" styleClass="blueButton" text="Cancelar">
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
            </Button>
            <Button fx:id="createLoginButton"  onMouseClicked="#createLoginAct"  layoutX="310.0" layoutY="360.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="175.0" styleClass="redButton" text="Criar Login">
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor>
            </Button>
            <Label layoutX="134.0" layoutY="14.0" prefHeight="35.0" prefWidth="350.0" styleClass="titleStd" text="   Registro de Usuario" textAlignment="CENTER" />
         </children></Pane>
    </children>


