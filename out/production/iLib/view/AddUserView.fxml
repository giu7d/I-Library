<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>

<AnchorPane prefHeight="480.0" prefWidth="720.0" styleClass="singupBG" stylesheets="@../assets/Style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddUserController">

    <children>
        <Label fx:id="closeButton" onMouseClicked="#closeAct" prefHeight="25.0" prefWidth="25.0" text="x">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Label>
        <Pane layoutX="23.0" layoutY="48.0" prefHeight="437.0" prefWidth="670.0" styleClass="singupPane">
            <children>
                <Text layoutX="222.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="titleStd" text="Cadastrar Usuario" textAlignment="CENTER" />
                <TextField fx:id="nameField" layoutX="204.0" layoutY="74.0" promptText="Nome" styleClass="textfieldLightBox" />
                <TextField fx:id="raField" layoutX="204.0" layoutY="136.0" promptText="RA" styleClass="textfieldLightBox" />
                <ComboBox fx:id="cursoCombo" layoutX="203.0" layoutY="259.0" prefHeight="38.0" prefWidth="261.0" promptText="Curso" styleClass="textfieldLightBox"></ComboBox>
                <ComboBox fx:id="ufCombo" layoutX="203.0" layoutY="200.0" prefHeight="38.0" prefWidth="261.0" promptText="Estado" styleClass="textfieldLightBox"></ComboBox>
                <Button fx:id="cancelButton" layoutX="205.0" layoutY="380.0" mnemonicParsing="false" onMouseClicked="#backAct" prefHeight="57.0" prefWidth="130.0" styleClass="blueButton" text="Cancelar">
                    <cursor>
                        <Cursor fx:constant="OPEN_HAND" />
                    </cursor></Button>
                <Button fx:id="addButton" onMouseClicked="#modAct" layoutX="334.0" layoutY="380.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="130.0" styleClass="redButton" text="Criar">
                    <cursor>
                        <Cursor fx:constant="OPEN_HAND" />
                    </cursor>
                </Button>
            </children>
        </Pane>
    </children>

</AnchorPane>
